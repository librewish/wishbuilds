# Maintainer: Guinux <nuxgui@gmail.com>

pkgname=garuda-release
pkgver=0.3
pkgrel=1
pkgdesc="garuda's release definition"
arch=("any")
url="https://garuda.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release' 'os-release')
install="garuda-release.install"
sha256sums=('SKIP'
	    'SKIP')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    mkdir -p ${pkgdir}/usr/lib
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/
    install -m644 ${srcdir}/os-release ${pkgdir}/usr/lib/

}
